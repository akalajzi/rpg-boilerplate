{
  "name": "rpg-boilerplate",
  "version": "1.0.0",
  "description": "My React-Parcel-GraphQL boilerplate",
  "main": "index.js",
  "license": "MIT",
  "author": "Ante Kalajzic <akalajzi@gmail.com",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel entry/index.html --out-dir dist/bundles",
    "build": "parcel build entry/index.html -d dist --detailed-report",
    "build-imported": "imported-components src etc/imported-chunk.js",
    "build:treeshake": "yarn build --experimental-scope-hoisting",
    "clean": "rimraf dist",
    "clean-production": "yarn run clean && yarn run production",
    "production": "cross-env NODE_ENV=production && yarn build",
    "flow:check": "flow check",
    "lint": "eslint .",
    "test": "jest",
    "check-all": "yarn flow:check && yarn lint && yarn test --coverage"
  },
  "dependencies": {
    "node-sass": "^4.9.3",
    "react": ">= 16.0.0 < 17.0.0-0",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-imported-component": "^4.6.2",
    "react-router-dom": "^4.3.1",
    "styled-components": "^3.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.7.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.81.0",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "jest-styled-components": "^6.2.0",
    "prettier": "^1.14.2",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.5.1",
    "rimraf": "^2.6.2"
  }
}
